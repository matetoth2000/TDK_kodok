    "import openai\n",
    "from collections.abc import Iterator\n",
    "import pandas as pd\n",
    "from tqdm.notebook import tqdm  # Use tqdm.notebook for Jupyter Notebooks\n",
    "\n",
    "openai.api_key = \""\n",
    "\n",
    "client = openai.OpenAI(\n",
    "    # This is the default and can be omitted\n",
    "    api_key=openai.api_key,\n",
    ")\n",
    "\n",
    "# Initialize a list to store replies\n",
    "replies = []\n",
    "\n",
    "# Iterate through the DataFrame with a progress bar\n",
    "for index, row in tqdm(feor_df.iterrows(), total=feor_df.shape[0]):\n",
    "    FEOR_08_kod = row['4_digit_FEOR_group_code']\n",
    "    FEOR_08_nev = row['4_digit_FEOR_group_title']\n",
    "    FEOR_08_desc = row['description']\n",
    "    FEOR_08_feladatok = row['tasks']\n",
    "    num_tasks = FEOR_08_feladatok.count(';') + 1\n",
    "    \n",
    "    # Construct the prompt\n",
    "    messages = [{\"role\": \"system\", \"content\": str(FEOR_08_nev) + \" vagy. Munkaköröd leírása: \" + str(FEOR_08_desc)},\n",
    "              {\"role\": \"user\", \"content\": \"Munkafeladataid ';' karakterrel elválasztva: \" + str(FEOR_08_feladatok) + f\" Add meg mind a(z) {num_tasks} darab munkafeladatod időarányos részesedését az összes munkaidődből, a következő formátumban (csak számok): 'munkafeladat1 aránya;munkafeladat2 aránya;...;munkafeladat{num_tasks} aránya'. \" + f\"Vedd úgy, hogy nincs más munkafeladatod felsoroltakon kívül, tehát az arányok összege pontosan 1 legyen. Ne adj vissza semmi mást, csak a {num_tasks} arányt ';' karakterrel elválasztva, két tizedesjegy pontossággal.\"}]\n",
    "\n",
    "\n",
    "    # Example of calling the GPT model\n",
    "    response = client.chat.completions.create(\n",
    "      model=\"gpt-4-turbo-preview\",\n",
    "      messages=messages,\n",
    "      temperature=0,\n",
    "      max_tokens=100\n",
    "    )\n",
    "    \n",
    "    # Extracting and storing the response\n",
    "    if response.choices:\n",
    "        reply = response.choices[0].message.content\n",
    "        replies.append(reply)\n",
    "    else:\n",
    "        # If no response is received, append a placeholder or None\n",
    "        replies.append(None)\n",
    "        \n",
    "# Assign the replies list as a new column in the DataFrame\n",
    "feor_df['task_share'] = replies"
   ]
  },
